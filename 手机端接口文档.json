            "k3": [{
                "name": "江苏快３", //彩种名称
                "image": "url...", //彩种LOGO
                "number": "80", //每天多少期数
            }],
            //所有--时时彩
            "ssc": [{
                "name": "江苏快３", //彩种名称
                "image": "url...", //彩种LOGO
                "number": "80", //每天多少期数
            }],
            //所有--快乐彩
            "klc": [{
                "name": "江苏快３", //彩种名称
                "image": "url...", //彩种LOGO
                "number": "80", //每天多少期数
            }],
